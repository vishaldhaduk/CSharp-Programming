        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Margin="10" VerticalAlignment="Top" HorizontalAlignment="Left" Orientation="Horizontal">
            <TextBox Width="300" Height="50"/>
            <Button Width="100" Margin="10,0,0,0" Click="Button_Click">Submit</Button>
        </StackPanel>
    </Grid>
</Window>
